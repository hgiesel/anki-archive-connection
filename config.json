{
  "debug": false,

  "archive_root": "/Users/hgiesel/Developer/archive",

  "regexes": {
    "pagerefs": "<<!?([^>,]+).*>>",
    "qidrefs": "^:ext:([0-9]+):(?: .*)?$",
    "headings": "^=(?: )?(.*)",
    "qids": "^:([0-9]+):(?: .*)?",
    "stats": [
      "^(\\.[^. ]+|\\.+ .+|\\*+ .+|[^\\n\\'\":=/\\-+< ]+)"
